    List<PrimalHunterModels.PrimalHunterItemModel> BundleGenerator(int[] ii)
    {
        List<PrimalHunterModels.PrimalHunterItemModel> temp = new();
        for (int i = 0; i < ii.Count(); i++)
        {
            var itemp = DataPrimalItemsList.Where(x => x.item_id == ii[i]).FirstOrDefault();
            if (itemp != null)
            {
                temp.Add(itemp);
            }
        }
        return temp;
    }
