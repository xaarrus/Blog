    {
        if (newPHSaveData.inventory.FirstOrDefault(x => x.guid == guidDelete) != null)
        {
            var deleteItem = newPHSaveData.inventory.FirstOrDefault(x => x.guid == guidDelete);
            newPHSaveData.inventory.Remove(deleteItem);
        }
    }
